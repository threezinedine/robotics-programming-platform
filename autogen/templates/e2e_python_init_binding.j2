from dataclasses import * # type: ignore

{% for class in classes %}
    {% if "e2e" in class.annotations %}
import {{ class.name }} # type: ignore
    {% endif %}
{% endfor %}

INVALID_ID = -1
FILE_READ = 0
FILE_WRITE = 1
FILE_APPEND = 2

{% for struct in structs %}
{% if struct.name in allJsonMappedClasses-%}
@dataclass
class {{ struct.name }}:
{% if struct.fields|length != 0 %}
{% for field in struct.fields %}
    {{ field.name }}: {{ convertCppTypeToPyType(field.type) }}
{% endfor %}
{% else-%}
    pass
{% endif-%}

{% endif-%}
{% endfor %}
